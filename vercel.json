{
  "version": 2,
  "builds": [
    {
      "src": "api/numbers/index.js",
      "use": "@vercel/node"
    },
    {
      "src": "api/numbers/[id].js",
      "use": "@vercel/node"
    },
    {
      "src": "api/check.js",
      "use": "@vercel/node"
    },
    {
      "src": "api/404.js",
      "use": "@vercel/node"
    },
    {
      "src": "api/auth/login.js",
      "use": "@vercel/node"
    }
  ],
  "rewrites": [
    {
      "source": "/api/numbers",
      "destination": "/api/numbers/index.js"
    },
    {
      "source": "/api/numbers/:id",
      "destination": "/api/numbers/[id].js"
    },
    {
      "source": "/api/check",
      "destination": "/api/check.js"
    },
    {
      "source": "/api/check/:number",
      "destination": "/api/check.js"
    },
    {
      "source": "/api/auth/login",
      "destination": "/api/auth/login.js"
    }
  ],
  "headers": [
    {
      "source": "/api/(.*)",
      "headers": [
        { "key": "Access-Control-Allow-Origin", "value": "*" },
        { "key": "Access-Control-Allow-Methods", "value": "GET,POST,PUT,DELETE,OPTIONS" },
        { "key": "Access-Control-Allow-Headers", "value": "Content-Type,Authorization" }
      ]
    }
  ]
}
